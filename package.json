{
    "name": "gee-mailer",
    "version": "1.0.0",
    "description": "A local-first email intelligence and analytics system",
    "module": "src/index.ts",
    "type": "module",
    "private": true,
    "keywords": [
        "gmail",
        "email",
        "analytics",
        "typescript",
        "bun"
    ],
    "author": "Angel Moreno <angel@angelxmoreno.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/angelxmoreno/gee-mailer.git"
    },
    "bugs": {
        "url": "https://github.com/angelxmoreno/gee-mailer/issues"
    },
    "homepage": "https://github.com/angelxmoreno/gee-mailer#readme",
    "scripts": {
        "prepare": "bun lefthook install",
        "dev": "bun run codegen:bullmq && biome check --write . && bun --hot src/index.ts",
        "build": "bun run codegen:bullmq && biome check --write . && tsc",
        "test": "bun test",
        "typecheck": "bun run codegen:bullmq && biome check --write . && tsc --noEmit",
        "lint": "biome check .",
        "lint:fix": "bun run codegen:bullmq && biome check --write .",
        "check": "bun run typecheck && bun run lint",
        "codegen:bullmq": "bun src/cli/bullmq-codegen.ts",
        "user:auth": "bun src/cli/auth.ts",
        "user:sync": "bun src/cli/sync.ts",
        "workers:start": "bun src/workers.ts",
        "workers:dev": "bun --hot src/workers.ts",
        "pm2:start": "pm2 start ecosystem.config.cjs",
        "pm2:start:dev": "pm2 start ecosystem.config.cjs --env development",
        "pm2:start:prod": "pm2 start ecosystem.config.cjs --env production",
        "pm2:stop": "pm2 stop ecosystem.config.cjs",
        "pm2:restart": "pm2 restart ecosystem.config.cjs",
        "pm2:delete": "pm2 delete ecosystem.config.cjs",
        "pm2:logs": "pm2 logs",
        "pm2:status": "pm2 status",
        "docker:start": "docker compose -f docker/docker-compose.dev.yml up -d",
        "docker:stop": "docker compose -f docker/docker-compose.dev.yml down",
        "docker:clear": "docker compose -f docker/docker-compose.dev.yml down -v",
        "docker:logs": "docker compose -f docker/docker-compose.dev.yml logs -f",
        "migration:create": "bash scripts/create-migration.sh",
        "migration:generate": "bash scripts/generate-migration.sh",
        "migration:show": "bun --bun typeorm migration:show -d src/database/dataSource.ts",
        "migration:run": "bun --bun typeorm migration:run -d src/database/dataSource.ts",
        "migration:revert": "bun --bun typeorm migration:revert -d src/database/dataSource.ts"
    },
    "dependencies": {
        "@keyvhq/core": "^2.1.11",
        "@keyvhq/redis": "^2.1.11",
        "@keyvhq/sqlite": "^2.1.11",
        "@ts-types/deep-partial": "^0.0.1",
        "@types/open": "^6.2.1",
        "bullmq": "^5.63.2",
        "eta": "^4.4.1",
        "googleapis": "^166.0.0",
        "inflection": "^3.0.2",
        "open": "^11.0.0",
        "pg": "^8.16.3",
        "pino": "^10.1.0",
        "pm2": "^6.0.13",
        "reflect-metadata": "^0.2.2",
        "tagged-keyv-wrapper": "^1.3.0",
        "ts-deepmerge": "^7.0.3",
        "tsyringe": "^4.10.0",
        "typeorm": "^0.3.27",
        "typeorm-pino-logger": "^0.2.0",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.3.6",
        "@commitlint/cli": "^20.1.0",
        "@commitlint/config-conventional": "^20.0.0",
        "@types/bun": "^1.3.2",
        "lefthook": "^2.0.4",
        "pino-pretty": "^13.1.2"
    },
    "peerDependencies": {
        "typescript": "^5.9.3"
    }
}
